{
  "name": "@phoobynet/alpaca-services",
  "version": "0.0.7",
  "description": "Alpaca API client",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run lint:fix && npm run clean && npm run build:types && npm run build:ts",
    "clean": "rimraf dist",
    "build:types": "tsc -p tsconfig-build.json --emitDeclarationOnly",
    "build:ts": "tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:config": "jest --showConfig",
    "workflow:test": "jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-rate-limit": "^1.3.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "eventemitter3": "^4.0.7",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.21",
    "ws": "^8.8.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phoobynet/alpaca-services.git"
  },
  "keywords": [
    "alpaca",
    "real-time",
    "api",
    "rest"
  ],
  "files": [
    "dist"
  ],
  "author": "phoobynet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phoobynet/alpaca-services/issues"
  },
  "homepage": "https://github.com/phoobynet/alpaca-services#readme"
}
